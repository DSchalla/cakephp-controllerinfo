{
    "name": "Schalla/ControllerInfo",
    "description": "ControllerInfo plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "ControllerInfo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ControllerInfo\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
